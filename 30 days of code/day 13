class MyBook extends Book{
    int Price;
    MyBook(String title,String author,int price)
    {
        super(title,author);
            this.Price=price;
    }
    void display()
    {
        System.out.println("Title:"+ " "+ this.title);
        System.out.println("Author:"+ " "+ this.author);  
        System.out.println("Price:"+ " "+ this.Price);        
    }
}
